		"typescript": "~4.7.4",
		"vite": "^5.1.5",
		"vitest": "^0.28.5"
	},
	"type": "module",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"better-sqlite3": "^10.0.0",
		"cron": "^3.1.6",
		"dayjs": "^1.11.10",
		"mega-session": "^1.1.2",
		"mysql2": "^3.9.2",
		"uuid": "^9.0.1",
		"zod": "^3.22.4"
